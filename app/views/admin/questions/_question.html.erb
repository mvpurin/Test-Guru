<tr>
  <td><%= question.body %><td>
  <td><%= link_to 'Answers', admin_question_answers_path(question) %></td>
  <td><%= link_to 'Edit', edit_admin_question_path(question), class: "btn btn-warning"%></td>
  <td><%= link_to 'Delete',
                  admin_question_path(question),
                  class: "btn btn-danger",
                  method: :delete,
                  data: {confirm: 'Are you sure?'} %>
  </td>
</tr>
