<div class="row justify-content-center mt-3">
  <div class="col col-md-4">
    <table class="table">
      <thead>
        <tr align="center">
          <th>Test title</th>
          <th>Level</th>
          <th>Questions</th>
          <th align="center", colspan="3">Action</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td align="center", rowspan="0"><%= @test.title%></td>
          <td align="center", rowspan="0"><%= @test.level%></td>
          <td><%= render @test.questions %></td>
        </tr>
      </tbody>
    </table>
    <%= link_to 'New question', new_admin_test_question_path %><br>
    <%= link_to 'Edit test', edit_admin_test_path(@test) %><br>
    <%= link_to 'Back', admin_tests_path %>
  </div>
</div>
